{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System.Net.Http.HttpClient.IEventServiceClient": "Warning"
    },
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    }
  },
  "WorkerConfig": {
    "GetNextWaitInSeconds": 3,
    "InfiniteRun": true,
    "DispatchRetryCountBeforeForcingConsume": 2,
    "RetryWaitInSeconds":  10
  },
  "ServiceUrls": {
    "AggregatorUrl": "http://localhost:8080",
    "EventUrl": "http://localhost:8081",
    "ProductUrl": "http://localhost:8082"
  }
}